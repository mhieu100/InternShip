    "antd": "^5.26.7",
    "async-mutex": "^0.5.0",
    "axios": "^1.11.0",
    "d3": "^7.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
